%{
#include "y.tab.h"   // include tokens generated by Yacc
%}

%%
[0-9]+          { yylval = atoi(yytext); return NUMBER; }
[ \t\n]         { /* skip whitespace */ }
"+"             { return '+'; }
"-"             { return '-'; }
"*"             { return '*'; }
"/"             { return '/'; }
"("             { return '('; }
")"             { return ')'; }
.               { return yytext[0]; }
%%

int yywrap() { return 1; }
